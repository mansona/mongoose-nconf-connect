{
    "env": {
      "node": true
    },
    "extends": "airbnb",
    "rules" : {
      "func-names": 0,
      "new-cap": [2 , { "capIsNewExceptions": ["Q", "ObjectId"] }],
      "no-underscore-dangle": [2, { "allow": ["_id"] }],
      "no-var": 0,
      "object-shorthand": 0,
      "prefer-arrow-callback": 0,
      "prefer-template": 0,
      "space-before-function-paren": 0
    }
}
